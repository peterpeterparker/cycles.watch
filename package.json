{
  "name": "cycles-watch",
  "version": "0.0.4",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "hack": "npm run dev -- --mode hack",
    "testnet": "npm run dev -- --mode testnet"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@sveltejs/adapter-static": "^1.0.0-next.41",
    "@sveltejs/kit": "^1.0.0-next.465",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.54.8",
    "svelte": "^3.50.0",
    "svelte-check": "^2.9.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "~4.8.2",
    "vite": "^3.1.0-beta.2"
  },
  "type": "module",
  "dependencies": {
    "@dfinity/agent": "^1.0.0-beta.0",
    "@dfinity/auth-client": "^1.0.0-beta.0",
    "@dfinity/authentication": "^1.0.0-beta.0",
    "@dfinity/candid": "^1.0.0-beta.0",
    "@dfinity/identity": "^1.0.0-beta.0",
    "@dfinity/principal": "^1.0.0-beta.0",
    "idb-keyval": "^6.2.0"
  }
}
