		"@sveltejs/kit": "^2.16.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"idb-keyval": "^6.2.1",
		"svelte": "^5.19.3",
		"svelte-check": "^4.1.4"
